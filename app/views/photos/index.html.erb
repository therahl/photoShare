<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>
<div class="grid">
<div id="masonry-container" class="transitions-enabled infinite-scroll clearfix">


  <% @photos.each do |photo| %>

        <div class="pic has-gutters">
          <%= image_tag photo.image.url(:medium), class: "img-responsive" %>
         <p>
            <%= link_to 'Show', album_photo_path(id: photo) %>
            <%= link_to 'Edit', edit_album_photo_path(id: photo) %>
            <%= link_to 'Destroy', album_photo_path(id: photo), method: :delete %>
         </p>
        </div>
    <% end %>
</div>
  <p class="test"></p>
<%= link_to 'Add Photos', new_album_photo_path %>
